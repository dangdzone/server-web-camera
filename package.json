    "@nestjs/websockets": "^10.2.7",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "env-cmd": "^10.1.0",
    "firebase-admin": "^11.11.0",
    "mongodb": "^6.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
